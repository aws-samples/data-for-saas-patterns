{
    "Version": "2012-10-17",
    "Statement": [{
        "Effect": "Allow",
        "Action": [
            "s3:GetObject",
            "s3:ListBucket"
        ],
        "Resource": [
            "arn:aws:s3:::#bucket_name#",
            "arn:aws:s3:::#bucket_name#/*"
        ],
        "Condition": {
            "StringEquals": {
                "aws:PrincipalAccount": "#account_id#"
            }
        }
    }]
}